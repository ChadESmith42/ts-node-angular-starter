{
    "compilerOptions": {
        "noImplicitAny": false,
        "noEmitOnError": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "removeComments": false,
        "sourceMap": true,
        "target": "es5",
        "outDir": "dist"
    },
    "include": [
        "modules/**/*",
        "utilities/**/*",
        "db/**/*",
        "index.ts",
    ]
}